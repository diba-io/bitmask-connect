  "license": "MIT",
  "dependencies": {
    "bitmask-core": "^0.7.0-rc.24",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "eslint": "^9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5"
  },
  "scripts": {
    "compile": "tsc",
    "publish": "yarn compile && cd dist && npm publish && cd ..",
    "lint": "yarn prettier --check . && yarn eslint src",
